{
  "presets": [
    [
      "@babel/preset-env",
      {
          "targets": {
            "chrome": "58",
            "ie": "8"
          },
          "modules": false
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "helpers": false,
        "useESModules": true,
        "absoluteRuntime": true
      }
    ],
    [
      "component",
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      }
    ]
  ]
}